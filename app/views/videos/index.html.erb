<div id='content'>
  <h1>Listing videos</h1>
  
  <table>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Category</th>
      <th>Subject</th>
      <th>Url</th>
  <!--    <th>Embed code</th>
      <th>Age level</th>
      <th>Rating</th>
      <th>Finder</th> -->
      <th></th>
      <th></th>
      <th></th>
    </tr>
  
  <% @videos.each do |video| %>
    <tr>
      <td><%= video.title %></td>
      <td><%= video.description %></td>
      <td><%= video.category %></td>
      <td><%= video.subject %></td>
      <td><%= link_to 'link', video.url, :target => "_blank" %></td>
  <!--    <td><%= video.embed_code %></td>
      <td><%= video.age_level %></td>
      <td><%= video.rating %></td>
      <td><%= video.finder_id %></td> -->
      <td><%= link_to 'Show', video %></td>
      <td><%= link_to 'Edit', edit_video_path(video) %></td>
      <td><%= link_to 'Destroy', video, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
  </table>
  
  <br />
  
  <%= link_to 'New Video', new_video_path %>
</div>